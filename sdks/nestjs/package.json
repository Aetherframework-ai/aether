{
	"name": "@aetherframework.ai/nestjs",
	"version": "1.0.0",
	"description": "NestJS SDK for Aether workflow engine",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"aether",
		"nestjs",
		"workflow",
		"orchestration"
	],
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage"
	},
	"dependencies": {
		"@nestjs/common": "^10.0.0",
		"@nestjs/core": "^10.0.0",
		"@nestjs/platform-express": "^10.0.0",
		"@nestjs/config": "^3.0.0",
		"@aetherframework.ai/sdk": "file:../sdk",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.0"
	},
	"devDependencies": {
		"@nestjs/testing": "^10.0.0",
		"@types/jest": "^29.5.0",
		"@types/node": "^18.0.0",
		"jest": "^29.0.0",
		"ts-jest": "^29.0.0",
		"typescript": "^5.0.0"
	},
	"peerDependencies": {
		"@nestjs/common": ">=10.0.0",
		"@nestjs/core": ">=10.0.0",
		"@aetherframework.ai/sdk": ">=0.1.0"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			[
				"@semantic-release/npm",
				{
					"pkgRoot": "."
				}
			],
			[
				"@semantic-release/github",
				{
					"assets": []
				}
			]
		]
	}
}
